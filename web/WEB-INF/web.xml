<?xml version="1.0" encoding="UTF-8"?>
<web-app version="3.1" xmlns="http://xmlns.jcp.org/xml/ns/javaee" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_3_1.xsd">
    <servlet>
        <servlet-name>Admin_controller</servlet-name>
        <servlet-class>controller.Admin_controller</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>Employer_controller</servlet-name>
        <servlet-class>controller.Employer_controller</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>Specialite_controller</servlet-name>
        <servlet-class>controller.Specialite_controller</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>Service_controller</servlet-name>
        <servlet-class>controller.Service_controller</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>Admin_controller</servlet-name>
        <url-pattern>/Admin_controller</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>Employer_controller</servlet-name>
        <url-pattern>/Employer_controller</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>Specialite_controller</servlet-name>
        <url-pattern>/Specialite_controller</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>Service_controller</servlet-name>
        <url-pattern>/Service_controller</url-pattern>
    </servlet-mapping>
    <session-config>
        <session-timeout>
            30
        </session-timeout>
    </session-config>
</web-app>
